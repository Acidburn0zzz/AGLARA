# send as genfic.local
# receive for mail.ip4.internal.genfic.local
mydomain = ip4.internal.genfic.local
myorigin = genfic.local
mydestination = $myhostname localhost.$mydomain localhost $mydomain genfic.local
# only accept internal incoming
mynetworks_style = subnet
mynetworks = 192.168.100.0/24 [2001:db8:81:e2::]/64
relay_domains =

home_mailbox = .maildir/
